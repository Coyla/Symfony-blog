{% extends 'DEVBlogBundle:Admin:admin_template.html.twig' %}

{% block admin_body %}
    admin section
    {% for post in posts %}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"> {{ post.title }}</h3>
            </div>
            <div class="panel-body">
                {{ post.content }}
            </div>
            <a type="button" href="" value="{{ post.id }}"> Publish </a>
        </div>

    {% endfor %}
{% endblock %}
